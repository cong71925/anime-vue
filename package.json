    "flv.js": "^1.6.2",
    "hls.js": "^1.4.3",
    "libass-wasm": "^4.1.0",
    "pinia": "^2.0.35",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@hrgui/libass-wasm-ts": "^1.0.2",
    "@rushstack/eslint-patch": "^1.2.0",
